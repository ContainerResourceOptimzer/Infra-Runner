      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "id": 2,
      "targets": [
        {
          "expr": "sum(k6_http_reqs_total{experiment=\"$expId\", container=\"$expId\"})",
          "legendFormat": "requests/s",
          "refId": "A"
        }
      ]
    },
    {
      "title": "HTTP Request Duration (p95)",
      "type": "graph",
      "datasource": "Prometheus",
      "gridPos": {
        "h": 9,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "id": 4,
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (le) (rate(k6_http_req_duration_seconds{experiment=\"$expId\", container=\"$jobId\"}[5m])))",
          "legendFormat": "p95 duration (ms)",
          "refId": "A"
        }
      ]
    }
